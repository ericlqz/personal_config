[alias]
	last = log -1 HEAD
	unstage = reset HEAD --
	st = status
	ci = commit
	co = checkout
    br = branch
	merge-nf = merge --no-ff
    slog = log --graph --full-history --all --pretty=format:"%h%x09%d%x20%s"
    graph = log --graph --color --all --pretty=format:"%C(yellow)%H%C(green)%d%C(reset)%n%x20%cd%n%x20%cn%x20(%ce)%n%x20%s%n"
